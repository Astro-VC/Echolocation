script/source = "extends Control

@export var map_id : int

func _ready() -> void:
	$Background/Life.text = str(map_id)

func _process(delta: float) -> void:
	if map_id == Resources.map_coord:
		$Background.show()
		$Outline.show()
		return
	$Background.hide()
"

